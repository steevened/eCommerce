    sdid = getQueryParam('sdid'),
    promoid = getQueryParam('promoid'),
    // Read from either top level cookie or from TID sub-cookie
    tid = cookie.get('TID'),
    tidPieces = [],    
    tidCookieDetails = {
      domain: domain,
      expires: 120, // 120 days
    };
  
  if (sdid ||
    trackingid ||
    promoid) {
  
    tid = tid || '--';
  
    tidPieces = tid.split('-');
  
    // trackingid
    tidPieces[0] = trackingid || tidPieces[0];

    // sdid
    tidPieces[1] = sdid || tidPieces[1];

    // promoid
    tidPieces[2] = promoid || tidPieces[2];

    // set the cookie
    cookie.set('TID', tidPieces.join('-'),tidCookieDetails );
  
  }
  
}(_satellite));
  
  
});</script><script>_satellite["_runScript2"](function(event, target, Promise) {
var w_sp=_satellite,C0004=w_sp.groupEnabled("C0004");C0004&&w_sp.track("trackConsent");
});</script></body></html>